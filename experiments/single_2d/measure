#! /usr/bin/env python

import subprocess


FORM_TYPE = "mass"
MESH_TYPE = "tri"
MESH_SIZE = 600
DEGREE = 1
REPEATS = 5
LOG_FNAME = "log.txt"
METADATA_FNAME = "metadata.csv"

cmd = ["assemble-form", FORM_TYPE, MESH_TYPE, str(MESH_SIZE), str(DEGREE), 
       str(REPEATS), LOG_FNAME, METADATA_FNAME]

subprocess.run(cmd)
