#! /usr/bin/env python

from fireperf import log
from fireperf.assemble import assemble_form


FORM_TYPE = "mass"
MESH_TYPE = "tri"
MESH_SIZE = (600, 600)
DEGREE = 1
USE_ACTION = True
REPEATS = 5

LOG_FNAME = "log.txt"


assemble_form(FORM_TYPE, MESH_TYPE, MESH_SIZE, DEGREE, USE_ACTION, REPEATS)
log.write(LOG_FNAME)
