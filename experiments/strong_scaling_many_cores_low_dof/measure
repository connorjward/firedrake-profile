#! /usr/bin/env python

from fireperf import experiments


N_CORESS = range(24, 33)
MESH_SIZE = 600

args = experiments.parse_args()
experiments.do_warm_start(True)

for n_cores in N_CORESS:
    experiments.do_experiment(mesh_size=MESH_SIZE, cwd=args.output_dir, 
                              np=n_cores)
